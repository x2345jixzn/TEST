    let midPosition = 24576; // 1 ‡¶°‡¶≤‡¶æ‡¶∞ ‡¶™‡ßç‡¶∞‡¶´‡¶ø‡¶ü‡ßá ‡¶™‡¶ú‡¶ø‡¶∂‡¶®
    let maxLoss = -2000;
    let maxProfit = 5000; // ‡¶°‡¶ø‡¶´‡¶≤‡ßç‡¶ü ‡¶Æ‡¶æ‡¶®
    
